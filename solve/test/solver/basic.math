name "a super simple example."

type pint
type s

time t : s

input A : pint
input B : pint
output C : pint
output C2 : pint
param PF : pint = 0.5

assume mag A in (-2,2) : pint
assume mag B  in (-2,2) : pint
assume mag C in (-20,20) : pint
assume mag C2 in (-100,100): pint

rel C =  B + (A*B)/PF + 0.5
rel deriv(C2,t) = (A*B)/PF + C + 0.3 with C2(0) = 0.5
