
type ms
type M 

output Adimer :unit
output Amonomer : unit

param kr1 : unit = 10
param kr2 : unit = 10
param KDrv : unit = 10

time t : ms 

%monomer, this should map to AFREE
rel ddt  Adimer = (-1*kr1*Adimer) + (kr1/KDrv)*Amonomer*Amonomer init 0
def Adimer mag=[0,1] M

%dimer, this should map to CFREE
rel ddt Amonomer = kr1*Adimer*2 - (kr1/KDrv)*Amonomer*Amonomer*2 init 0
def Amonomer mag=[0,1] M
