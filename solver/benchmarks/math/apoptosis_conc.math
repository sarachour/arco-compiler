rel deriv(BiPT,t) = ( rxn_20) + (-rxn_21) with BiPT(0) = 0
rel deriv(ATF6T,t) = ( rxn_5) + (-rxn_6) + (-rxn_8) + (-rxn_w1) with ATF6T(0) = 0
