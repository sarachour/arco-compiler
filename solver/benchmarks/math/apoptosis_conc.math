rel deriv(ATF6p50,t) = ( rxn_10) + (-rxn_11) with ATF6p50(0) = 0
rel deriv(mWFS1,t) = ( rxn_w2) + (-rxn_w3) with mWFS1(0) = 0
rel deriv(WFS1,t) = ( rxn_w4) + (-rxn_w5) with WFS1(0) = 0
rel deriv(ATF4,t) = ( rxn_23) + (-kdATF4*ATF4) with ATF4(0) = 0
rel deriv(mCHOP,t) = ( rxn_25) + (-kdmCHOP*mCHOP) with mCHOP(0) = 0
rel deriv(CHOP,t) = ( ktrCHOP*mCHOP) + (-kdCHOP*CHOP) with CHOP(0) = 0
